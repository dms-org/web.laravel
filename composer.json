{
    "name": "dms-org/web.laravel",
    "description": "The integration package with the laravel framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Elliot Levin",
            "email": "elliotlevin@hotmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Dms\\Web\\Laravel\\": "./src/"
        },
        "files": [
            "./src/helpers.php"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/iddigitalagency/oauth2-microsoft",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/Chris53897/cache"
        }
    ],
    "require": {
        "php": "^8.2",
        "dms-org/core": "dev-upgrade",
        "dms-org/common.structure": "dev-upgrade",
        "dms-org/package.analytics": "dev-upgrade",
        "dms-org/library.metadata": "dev-upgrade",
        "laravel/framework": "^11.0",
        "marcj/topsort": "^1.0.0",
        "cache/cache": "dev-master",
        "league/oauth2-google": "^3.0.0",
        "stevenmaguire/oauth2-microsoft": "dev-master"
    },
    "autoload-dev": {
        "psr-4": {
            "Dms\\Web\\Laravel\\Tests\\": "./tests/Tests/",
            "Dms\\Core\\Tests\\": "./vendor/dms-org/core/tests/Tests/"
        }
    },
    "require-dev": {
        "dms-org/common.testing": "dev-upgrade",
        "laravel/browser-kit-testing": "^7.0",
        "orchestra/testbench": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dms\\Web\\Laravel\\DmsServiceProvider"
            ]
        }
    },
    "replace": {
        "symfony/http-foundation": "*",
        "league/flysystem": "*"
    }
}
